{
    "presets": [
        "es2015",
        "flow",
        "react",
        "stage-0",
        "react-optimize"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ],
    "env": {
        "production": {
            "plugins": [
                "transform-react-remove-prop-types",
                "transform-runtime"
            ]
        }
    }
}
